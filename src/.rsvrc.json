{
    "tempDir": "storage/temp",
    "staticFileDir": ["/"],
    "cacheControl": "no-cache, max-age=86400",
    "encoding": "latin1",
    "maxBufferSize": 50000000,

    "mimeTypes": {
        "json": "application/json",
        "html": "text/html",
        "xml": "text/xml",
        "js": "text/javascript",
        "css": "text/css",
        "jpg": "image/jpeg",
        "jpeg": "image/jpeg",
        "png": "image/png",
        "mp3": "audio/mp3",
        "mp4": "video/mp4",
        "pdf": "application/pdf"
    },
    "defaultDocuments": [
        "index.html"
    ],
    "httpErrors": {
        "baseDir": "",
        "404": ""
    }
}